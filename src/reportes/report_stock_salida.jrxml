	<queryString>
		<![CDATA[SELECT
     stock.`idStock` AS stock_idStock,
     stock.`cantidad` AS stock_cantidad,
     stock.`fecha` AS stock_fecha,
     stock.`salida` AS stock_salida,
     stock.`fk_idproductos` AS stock_fk_idproductos,
     productos.`idproductos` AS productos_idproductos,
     productos.`producto` AS productos_producto,
     ventas.`idVentas` AS ventas_idVentas,
     detallevta.`idDetalleVta` AS detallevta_idDetalleVta,
     ventas.`fk_idOperadores` AS ventas_fk_idOperadores,
     operadores.`razonSocial` AS operadores_razonSocial
FROM
     `productos` productos INNER JOIN `stock` stock ON productos.`idproductos` = stock.`fk_idproductos`
     INNER JOIN `detallevta` detallevta ON productos.`idproductos` = detallevta.`fk_idproductos`
     AND detallevta.`idDetalleVta` = stock.`fk_idDetalleVTA`
     INNER JOIN `ventas` ventas ON detallevta.`fk_idventas` = ventas.`idVentas`
     INNER JOIN `operadores` operadores ON ventas.`fk_idOperadores` = operadores.`idOperadores`
WHERE
     stock.`fk_idproductos` = $P{id}
ORDER BY
     stock.`fecha` DESC]]>
	</queryString>
